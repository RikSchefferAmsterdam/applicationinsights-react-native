        "grunt-run": "^0.8.1",
        "@nevware21/grunt-ts-plugin": "^0.4.3",
        "grunt-tslint": "^5.0.2",
        "qunit": "^2.11.2",
        "react": "16.13.1",
        "react-native": "0.64.2",
        "react-native-device-info": "^5.6.5",
